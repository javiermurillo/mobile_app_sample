<div data-role="page" data-add-back-btn="true">

  <div data-role="header" data-position="inline">
    <%= link_to "Back", :action => :index %>
    <h1><%= @inventory.brand %></h1>
    <%= link_to "Edit", :action => :edit, :id => @inventory.object %>
  </div>

  <div data-role="content">
    <table>
      
      <tr>
    	<td class="itemLabel">Brand</td>
        <td class="itemValue"><%= @inventory.brand%></td>
      </tr>
      
      <tr>
    	<td class="itemLabel">Product</td>
        <td class="itemValue"><%= @inventory.product%></td>
      </tr>
      
      <tr>
    	<td class="itemLabel">Qty</td>
        <td class="itemValue"><%= @inventory.qty%></td>
      </tr>
      
    </table>
  </div>

</div>
